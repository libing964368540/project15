.sl_edit{
    font-size: 17px;
      color: #929292;
margin-bottom: 10px;
margin-top: 5px;
}
.headImgWrap{
  width: 60px;
  height: 60px;
border-radius: 50%;
overflow: hidden;
}
.headImgWrap .img{
  width: 100%;
  height: 100%;
}
.sl_info{
  line-height: 45px;
  border-bottom: 1px solid #f3f5f8;
  padding-top:40px;
  color: #101010;
  font-size:16px;
  position: relative;
}
.sl_sex::before{
  content: '性别'
}
.sl_grate::before{
   content: '毕业年份'
}
.sl_classgrade::before{
    content: '毕业班级'
}
.sl_sex::before,.sl_grate::before,.sl_classgrade::before{
  position: absolute;
  left:0;
  top:10px;
  color: #929292;
  font-size: 14px;
}









